{
  "env": {
      "browser": true,
      "node": true,
      "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "rules": {
    "global-require": "off",
    "no-cond-assign": "off",
    "no-empty": "off"
  },
  "globals": {
    "cba": true,
    "browser": true,
    "chrome": true
  }
}
